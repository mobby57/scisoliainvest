# Configuration Frontend - Environnement Staging - EXEMPLE
# Copiez ce fichier vers .env.staging et configurez les valeurs

# API Backend Staging
NEXT_PUBLIC_API_URL=http://localhost:5001
VITE_API_URL=http://localhost:5001/api

# MQTT Topic (si utilisé)
NEXT_PUBLIC_MQTT_TOPIC=solia_invest_staging/#

# Environment
NODE_ENV=staging

# Port Frontend (différent de dev)
FRONTEND_PORT=5174

# Azure Configuration (peut utiliser les mêmes que dev ou des credentials dédiés)
AZURE_CLIENT_ID=STAGING_AZURE_CLIENT_ID_CHANGE_THIS
AZURE_TENANT_ID=STAGING_AZURE_TENANT_ID_CHANGE_THIS
AZURE_CLIENT_SECRET=STAGING_AZURE_CLIENT_SECRET_CHANGE_THIS

# Feature Flags pour Staging
NEXT_PUBLIC_ENABLE_DEBUG=true
NEXT_PUBLIC_ENABLE_BETA_FEATURES=true
VITE_ENV=staging

# Monitoring (optionnel)
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_ERROR_REPORTING=true
