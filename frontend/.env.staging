# Frontend - Configuration Staging
# Variables d'environnement pour l'environnement de staging

# API Configuration
NEXT_PUBLIC_API_URL=https://staging.soliainvest.com/api
VITE_API_URL=https://staging.soliainvest.com/api

# Environment
NODE_ENV=staging
VITE_ENV=staging

# MQTT Configuration (si utilisé)
NEXT_PUBLIC_MQTT_TOPIC=solia_invest_staging/#

# Frontend Port (pour développement local staging)
FRONTEND_PORT=5174

# Azure AD B2C (staging)
AZURE_CLIENT_ID=REPLACE_WITH_STAGING_CLIENT_ID
AZURE_TENANT_ID=REPLACE_WITH_STAGING_TENANT_ID
AZURE_CLIENT_SECRET=REPLACE_WITH_STAGING_CLIENT_SECRET

# Feature Flags
NEXT_PUBLIC_ENABLE_DEBUG=true
NEXT_PUBLIC_ENABLE_ANALYTICS=true

# App Configuration
NEXT_PUBLIC_APP_NAME=SCI Solia Invest - Staging
NEXT_PUBLIC_APP_VERSION=staging
