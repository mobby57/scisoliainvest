    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "canvas": "^3.2.0",
    "cypress": "^14.5.3",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
