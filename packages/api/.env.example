# AWS Configuration
AWS_REGION=eu-west-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key

# IoT Core Configuration
IOT_CORE_ENDPOINT=your-iot-endpoint.iot.eu-west-1.amazonaws.com

# Timestream Configuration
TIMESTREAM_DATABASE=sci-solia-iot
TIMESTREAM_TABLE=sensor-data

# Amazon Q Configuration (Phase 2)
AMAZON_Q_APPLICATION_ID=your-q-app-id
AMAZON_Q_INDEX_ID=your-q-index-id

# Redis Configuration
REDIS_URL=redis://127.0.0.1:6379
# DISABLE_REDIS=true  # Uncomment to disable Redis and use in-memory cache