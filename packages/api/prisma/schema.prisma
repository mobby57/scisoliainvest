// Prisma Schema for SCI Solia Invest API

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Property {
  id          String   @id @default(uuid())
  name        String
  address     String
  description String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
